    <validationRules>
        <fullName>Status_Admitido_cuando_es_candidato</fullName>
        <active>true</active>
        <description>Regla de validación cuando el usuario realice el cambio el estado admisión y dicho registro se encuentre siendo candidato muestre el error.</description>
        <errorConditionFormula>AND(ISPICKVAL(hed__Application_Status__c ,&apos;Admitido&apos;), ISBLANK( hed__Applicant__c))</errorConditionFormula>
        <errorMessage>No esta permitido el estado en Admisión si el registro es un candidato</errorMessage>
    </validationRules>
